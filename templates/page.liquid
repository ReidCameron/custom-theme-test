<div class="page-width">
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      <div class="section-header text-center">
        <h1>{{ page.title }}</h1>
      </div>

      <div class="rte">
        <div class="grid__item" >
        {{ page.content }}
        </div>
         
        {% if page.handle == 'design-template' %}
        	{% include 'design-template' %} 
        {% endif %}
      </div>
    </div>
  </div>
</div>
